{
  "globs": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.d.ts",
    "**/*.mts",
    "**/*.cts",
    "**/tsconfig.json",
    "**/tsconfig.*.json",
    "**/.eslintrc*",
    "**/.prettierrc*",
    "**/package.json",
    "**/package-lock.json"
  ],
  "description": "TypeScript project files",
  "languageFeatures": [
    "Type annotations",
    "Interfaces and types",
    "Generics",
    "Decorators",
    "Strict type checking"
  ]
}